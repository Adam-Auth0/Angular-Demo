{
  "name": "login-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host app.acmecorp.com --disable-host-check --port 3000 --ssl true --sslCert ssl/acmecorp.com.crt --sslKey ssl/acmecorp.com.key --disableHostCheck true",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:app": "node server.js",
    "server:api": "node api-server.js",
    "dev": "npm-run-all --parallel start server:api",
    "prod": "npm-run-all --parallel server:app server:api",
    "postbuild": "npm run copy:server",
    "copy:server": "cp server.js auth_config.json dist/login-demo"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "~8.0.2",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@auth0/angular-jwt": "^3.0.1",
    "@auth0/auth0-spa-js": "^1.8.2",
    "@types/highlight.js": "^9.12.4",
    "@types/jsonwebtoken": "^8.5.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^5.3.3",
    "helmet": "^3.22.0",
    "jwks-rsa": "^1.8.0",
    "morgan": "^1.10.0",
    "rxjs": "^6.5.5",
    "typings": "^2.1.1"
  },
  "devDependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.42",
    "angular-ide": "^0.9.74",
    "codelyzer": "^5.2.2",
    "core-js": "3.1.3",
    "highlight.js": "^9.18.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ngx-highlightjs": "^3.0.3",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.4",
    "ts-node": "~8.2.0",
    "tslib": "~1.10.0",
    "tslint": "5.17.0",
    "typescript": "~3.4.5",
    "zone.js": "~0.9.1"
